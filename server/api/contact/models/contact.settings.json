{
  "kind": "singleType",
  "collectionName": "contacts",
  "info": {
    "name": "Contato",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nome": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Cidade": {
      "type": "string"
    },
    "Assunto": {
      "type": "text"
    },
    "Numero": {
      "type": "string",
      "regex": "^\\(?[1-9]{2}\\)? ?(?:[2-8]|9[1-9])[0-9]{3}\\-?[0-9]{4}$"
    }
  }
}
